<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wanglibao.huijiayou.activity.MainActivity">

    <FrameLayout
        android:id="@+id/fl_mainActivity_fragmentShell"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/bottom_navigation_bar">
    </FrameLayout>

    <com.ashokvarma.bottomnavigation.BottomNavigationBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:bnbMode="mode_fixed"
        app:bnbBackgroundColor="@color/white"
        app:bnbActiveColor="#E2BC85"
        app:bnbInactiveColor="#E2BC85"
        app:paddingStart="10dp"
        android:id="@+id/bottom_navigation_bar"
        android:layout_alignParentBottom="true" />
</RelativeLayout>
